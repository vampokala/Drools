		fields.add( assignmentGroup );
		fields.add( email );
		fields.add( address1 );
		fields.add( address2 );
		fields.add( city );
		fields.add( state );
		fields.add( postalCode );
		fields.add( contractNumber );
		fields.add( loginID );
		fields.add( NIPR );
		fields.add( firmName );
		fields.add( confirmationNumber );
		fields.add( agentCategory );
		fields.add( description );
		fields.add( phoneCallRequested );
		fields.add( agencyName );
